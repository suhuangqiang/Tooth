<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tooth.Activity.EncyclopediasActivity">
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include layout="@layout/titlebar2"/>
        <in.srain.cube.views.ptr.PtrClassicFrameLayout
            android:id="@+id/activity_encyclopedias_pcf"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ListView
                android:id="@+id/activity_encyclopedias_lv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:dividerHeight="20px"
                android:divider="@color/bg_color"></ListView>
        </in.srain.cube.views.ptr.PtrClassicFrameLayout>

    </com.zhy.autolayout.AutoLinearLayout>
</RelativeLayout>
